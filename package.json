{
  "name": "@ariel_jhy/ai-bot-adk",
  "version": "1.0.6",
  "type": "module",
  "description": "智能客服SDK - 基于Vue 3的移动端H5智能客服组件",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "keywords": [
    "chatbot",
    "ai",
    "sdk",
    "vue3",
    "customer-service",
    "智能客服",
    "h5",
    "mobile",
    "chat"
  ],
  "main": "./dist/chat-sdk.umd.js",
  "module": "./dist/chat-sdk.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chat-sdk.es.js",
      "require": "./dist/chat-sdk.umd.js"
    },
    "./style.css": "./dist/ai-bot-adk.css"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "rm -rf dist && npm run build:lib:es && npm run build:lib:umd",
    "build:lib:es": "vite build --mode lib:es",
    "build:lib:umd": "vite build --mode lib:umd",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,vue,css,scss,less,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css,scss,less,md,json}\"",
    "prepublishOnly": "npm run build:lib",
    "prepack": "npm run build:lib",
    "version": "node scripts/update-changelog.cjs && git add CHANGELOG.md",
    "preversion": "npm run build:lib",
    "release": "node scripts/release.cjs patch",
    "release:minor": "node scripts/release.cjs minor",
    "release:major": "node scripts/release.cjs major",
    "prep": "node scripts/prep-release.cjs patch",
    "prep:minor": "node scripts/prep-release.cjs minor",
    "prep:major": "node scripts/prep-release.cjs major",
    "publish": "node scripts/publish.cjs"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "less": "4.5.1",
    "prettier": "^3.4.2",
    "terser": "5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.1.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
